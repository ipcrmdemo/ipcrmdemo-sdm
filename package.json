{
  "name": "ipcrmdemo-sdm",
  "version": "0.1.0",
  "description": "my new project",
  "author": "matt",
  "license": "Apache-2.0",
  "homepage": "https://github.com/ipcrmdemo/ipcrmdemo-sdm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipcrmdemo/ipcrmdemo-sdm.git"
  },
  "bugs": {
    "url": "https://github.com/ipcrmdemo/ipcrmdemo-sdm/issues"
  },
  "keywords": [
    "atomist",
    "automation",
    "sdm",
    "seed",
    "spring",
    "spring-boot"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@atomist/automation-client": "1.5.0-master.20190515195848",
    "@atomist/automation-client-ext-dashboard": "^1.0.2",
    "@atomist/clj-editors": "0.5.7",
    "@atomist/microgrammar": "1.2.1-master.20190320034523",
    "@atomist/sdm": "1.5.0-master.20190517174921",
    "@atomist/sdm-core": "1.5.0-master.20190517230436",
    "@atomist/sdm-pack-analysis": "0.1.0-master.20190308084751",
    "@atomist/sdm-pack-analysis-dotnet": "0.1.0-master.20190506042825",
    "@atomist/sdm-pack-build": "^1.0.4",
    "@atomist/sdm-pack-changelog": "1.0.2-master.20181210135742",
    "@atomist/sdm-pack-cloudfoundry": "1.0.2",
    "@atomist/sdm-pack-docker": "1.1.0-master.20190213113315",
    "@atomist/sdm-pack-ecs": "0.0.2",
    "@atomist/sdm-pack-fingerprints": "2.0.5",
    "@atomist/sdm-pack-issue": "1.1.1-master.20181213113748",
    "@atomist/sdm-pack-k8s": "^1.5.1",
    "@atomist/sdm-pack-node": "1.0.4-master.20190321220053",
    "@atomist/sdm-pack-sonarqube": "1.0.2-master.20190320201553",
    "@atomist/sdm-pack-spring": "2.0.0-master.20190429120158",
    "@atomist/slack-messages": "^1.1.0",
    "@ipcrmdemo/sdm-pack-jira": "0.0.1-ipcrm-mappings-to-preferences.201905291119",
    "@kubernetes/client-node": "^0.8.2",
    "@types/dateformat": "^1.0.1",
    "@types/git-url-parse": "^9.0.0",
    "@types/lodash": "^4.14.108",
    "axios": "^0.19.0-beta.1",
    "dateformat": "^3.0.3",
    "docker-file-parser": "^1.0.4",
    "git-url-parse": "^11.1.2",
    "lodash": "^4.17.10",
    "ts-essentials": "^2.0.7",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@atomist/sdm-local": "1.0.2",
    "@types/fs-extra": "^5.0.2",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.7.1",
    "@types/power-assert": "^1.4.29",
    "@types/uuid": "^3.4.3",
    "espower-typescript": "^9.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.4.4",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supervisor": "^0.12.0",
    "ts-node": "7.0.1",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s compile test lint doc",
    "clean": "run-p clean:compile clean:test clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js{,.map}}\" \"{lib,test}/**/*.{d.ts,js{,.map}}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "clean:test": "rimraf .nyc_output coverage",
    "compile": "run-s git:info gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "git:info": "atm-git-info",
    "gql:gen": "atm-gql-gen",
    "lint": "tslint --format verbose --project . --exclude \"node_modules/**\" --exclude \"**/*.d.ts\" \"**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "atm-start",
    "test": "mocha --require espower-typescript/guess --require source-map-support/register \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc"
  },
  "engines": {
    "node": ">=8.1.0",
    "npm": ">=5.0.0"
  },
  "nyc": {
    "include": [
      "lib/**/*.*"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "lib/typings",
      "**/*.d.ts"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "all": true
  }
}
