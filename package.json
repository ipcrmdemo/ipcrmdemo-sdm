  },
  "keywords": [
    "atomist",
    "automation",
    "sdm",
    "seed",
    "spring",
    "spring-boot"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@atomist/automation-client": "^1.7.0-master.20190909084408",
